{
  "name": "@repo/ui-react",
  "version": "0.5.3",
  "author": "Festus Yeboah <festus.yeboah@hotmail.com>",
  "license": "MIT",
  "description": "A comprehensive React component library built on Web Loom UI ecosystem with headless behaviors and design system integration",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.cjs",
      "default": "./dist/components/*/index.js"
    },
    "./styles": "./dist/ui-react.css",
    "./styles/*": "./dist/styles/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "INTEGRATION.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint .",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006 --ci",
    "storybook:build": "storybook build"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@turbo/gen": "^2.6.1",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.33.0",
    "jsdom": "^26.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^8.4.7",
    "typescript": "5.8.2",
    "vite": "^6.1.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@web-loom/design-core": "*",
    "@web-loom/forms-core": "*",
    "@web-loom/forms-react": "*",
    "@web-loom/ui-core": "*",
    "@web-loom/ui-patterns": "*",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "react",
    "react-components",
    "ui-library",
    "design-system",
    "web-loom",
    "headless-ui",
    "component-library",
    "typescript",
    "accessible"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bretuobay/web-loom.git",
    "directory": "packages/ui-react"
  },
  "bugs": {
    "url": "https://github.com/your-org/web-loom/issues"
  },
  "homepage": "https://github.com/your-org/web-loom/tree/main/packages/ui-react#readme"
}
